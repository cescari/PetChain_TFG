  "source": "pragma solidity ^0.4.24;\n\ncontract Ownable {\n    address private owner;\n\n    constructor() public {\n        owner = msg.sender;\n    }\n    \n    modifier isOwner(){\n        require( owner == msg.sender);\n        _;\n    }\n}",
